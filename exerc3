int main()
{
    //float impar = 0, par = 0;
    int a[10];
    //int b[10];
    int i;
    for(i=0; i<10; i++){
        printf("Entre com o numero para a posicao %d do vetor A \n", i+1);
        scanf("%d", &a[i]);
    }
    
    for(i=9; i>=0; i--){
        printf("valor %d na posicao %d do vetor a \n  ", a[i], i+1);
        }
    return 0;
}
